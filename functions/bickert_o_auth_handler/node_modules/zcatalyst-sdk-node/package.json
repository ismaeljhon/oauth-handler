{
	"name": "zcatalyst-sdk-node",
	"version": "0.0.22",
	"description": "Node.js SDK for Zoho Catalyst",
	"main": "lib/index.js",
	"types": "./lib/index.d.ts",
	"scripts": {
		"build": "gulp build",
		"test:unit": "jest --config jestconfig.json",
		"format:src": "prettier --config \".prettierrc\" --write \"src/**/*.ts\" \"src/**/*.js\"",
		"format:unit": "prettier --config \".prettierrc\" --write \"test/**/*.ts\" \"test/**/*.js\"",
		"lint:src": "tslint -c tslint.json --format stylish -p tsconfig.json --exclude test/**/* --fix",
		"lint:unit": "tslint -c tslint.json --format stylish -p tsconfig.json test/*.ts test/**/*.ts -q --fix",
		"format": "run-p format:src format:unit",
		"lint": "run-p lint:src lint:unit",
		"test": "run-s format:unit lint:unit test:unit",
		"prepare": "npm run build",
		"prepublishOnly": "npm test && npm run lint",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src test",
		"postversion": "git push && git push --tags"
	},
	"engines": {
		"node": ">=8.13.0"
	},
	"author": "Catalyst (https://catalyst.zoho.com/)",
	"homepage": "https://catalyst.zoho.com/",
	"license": "MIT",
	"devDependencies": {
		"@types/jest": "^24.0.25",
		"@types/node": "^13.1.4",
		"gulp": "^4.0.2",
		"gulp-typescript": "^6.0.0-alpha.1",
		"jest": "^24.9.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.0",
		"ts-jest": "^24.3.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-config-standard": "^9.0.0",
		"tslint-plugin-prettier": "^2.1.0",
		"typescript": "^3.7.4"
	},
	"dependencies": {
		"combined-stream": "^1.0.8"
	},
	"files": [
		"lib/**/*"
	]
}
